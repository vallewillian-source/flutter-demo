{
    "service_name": "pinkapp",
    "auth_type": "BEARER_TOKEN",
    "login_endpoint": {
        "url": "https://staging.heypink.com/api/login",
        "method": "POST",
        "in_params": [
            {
                "address": "username",
                "name": "Login",
                "is_required": true 
            },
            {
                "address": "password",
                "name": "Senha",
                "is_required": true 
            }
        ],
        "out_params": {
            "auth_token": "data.authToken",
            "auth_user_id": "data.userId"
        }
    }
}